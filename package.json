{
  "name": "@jlongyam/studio",
  "version": "1.0.0",
  "description": "Jlongyam Studio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/studio.git"
  },
  "homepage": "https://github.com/jlongyam/studio",
  "keywords": [
    "SDK",
    "studio"
  ],
  "author": "jlongyam",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "transpile": "babel src -d lib"
  },
  "workspaces": [
    "project/*"
  ],
  "dependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-modules-systemjs": "^7.28.5",
    "@babel/preset-env": "^7.28.6",
    "fs-extra": "^11.3.3",
    "sync-directory": "^6.0.5",
    "systemjs": "^6.15.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "chrome 50"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-modules-systemjs",
        {
          "systemGlobal": "System"
        }
      ]
    ]
  }
}
