{
  "name": "studio",
  "version": "1.0.1",
  "description": "Jlongyam Studio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/studio.git"
  },
  "homepage": "https://github.com/jlongyam/studio",
  "keywords": [
    "SDK",
    "studio"
  ],
  "author": "jlongyam",
  "license": "MIT",
  "main": "main.js",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^25.2.1",
    "nw": "^0.107.0"
  },
  "bin": {
    "studio": "./scripts/studio.js"
  },
  "workspaces": [
    "packages/global/tester",
    "packages/global/env",
    "packages/global/ansi",
    
    "packages/global/fs",
    "packages/global/legacy"
  ],
  "dependencies": {
    "clean-modules": "^3.1.1",
    "es-check": "^9.5.4"
  },
  "scripts": {
    "package:tester:test": "npm test -w=@jlongyam/tester",
    "package:tester:build": "npm run build -w=@jlongyam/tester",
    "package:tester:build:minify": "npm run build:minify -w=@jlongyam/tester",
    "package:tester:check:iife": "node ./scripts/es-check.js ./packages/global/tester/dist/iife/tester.js ./packages/global/tester/dist/iife/assert.js",
    "package:tester:check:cjs": "node ./scripts/es-check.js ./packages/global/tester/dist/cjs/tester.js ./packages/global/tester/dist/cjs/assert.js",
    "package:tester:server": "npm run server -w=@jlongyam/tester",
    "clean": "npm prune && npm i && clean-modules && node ./scripts/clean.cjs"
  }
}
