{
  "name": "@jlongyam/tester",
  "version": "1.0.0",
  "description": "Tool for test",
  "keywords": [
    "test",
    "tool"
  ],
  "author": "jlongyam",
  "license": "MIT",
  "homepage": "https://github.com/jlongyam/studio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlongyam/studio.git",
    "directory": "packages/global/terser"
  },
  "main": "./dist/cjs/tester.js",
  "devDependencies": {
    "@babel/preset-env": "^7.29.0",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "es-toolkit": "^1.44.0",
    "http-server": "^14.1.1",
    "rollup": "^4.57.1",
    "rollup-plugin-polyfill-node": "^0.13.0"
  },
  "scripts": {
    "test:mjs:base": "node ./test/base.test.mjs",
    "test:mjs:assert": "node ./test/assert.test.mjs",
    "test:mjs:both": "node ./test/both.test.mjs",
    "test:mjs": "npm run test:mjs:base && npm run test:mjs:assert && npm run test:mjs:both",
    "build": "rollup -c",
    "build:minify": "rollup -c --config-min",
    "test:cjs:base": "node ./test/base.test.js",
    "test:cjs:assert": "node ./test/assert.test.js",
    "test:cjs:both": "node ./test/both.test.js",
    "test:cjs": "npm run test:cjs:base && npm run test:cjs:assert && npm run test:cjs:both",
    "server": "http-server -c-1 --no-dotfiles",
    "test": "npm run test:mjs && npm run test:cjs"
  },
  "exports": {
    ".": {
      "import": "./dist/es/tester.js",
      "require": "./dist/cjs/tester.js"
    },
    "./assert": {
      "import": "./dist/es/assert.js",
      "require": "./dist/cjs/assert.js"
    }
  },
  "browser": {
    ".": "./dist/iife/tester.js",
    "./assert": "./dist/iife/tester.js"
  },
  "files": [
    "./README.md",
    "dist",
    "test",
    "LICENSE"
  ],
   "engines": {
    "node": "^v5.12.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
